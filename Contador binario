int bit1=13;
int bit2=12;
int bit3=11;
int bit4=10;
int tiempo=1000;

void setup(){
  pinMode(bit1,OUTPUT);
  pinMode(bit2,OUTPUT);
  pinMode(bit3,OUTPUT);
  pinMode(bit4,OUTPUT);
}

void loop(){
  digitalWrite(bit1,LOW);
  digitalWrite(bit2,LOW);
  digitalWrite(bit3,LOW);
  digitalWrite(bit4,LOW);
  delay(tiempo);
  digitalWrite(bit1,HIGH);
  digitalWrite(bit2,LOW);
  digitalWrite(bit3,LOW);
  digitalWrite(bit4,LOW);
  delay(tiempo);
  digitalWrite(bit1,LOW);
  digitalWrite(bit2,HIGH);
  digitalWrite(bit3,LOW);
  digitalWrite(bit4,LOW);
  delay(tiempo);
  digitalWrite(bit1,HIGH);
  digitalWrite(bit2,HIGH);
  digitalWrite(bit3,LOW);
  digitalWrite(bit4,LOW);
  delay(tiempo);
  digitalWrite(bit1,LOW);
  digitalWrite(bit2,LOW);
  digitalWrite(bit3,HIGH);
  digitalWrite(bit4,LOW);
  delay(tiempo);
  digitalWrite(bit1,HIGH);
  digitalWrite(bit2,LOW);
  digitalWrite(bit3,HIGH);
  digitalWrite(bit4,LOW);
  delay(tiempo);
  digitalWrite(bit1,LOW);
  digitalWrite(bit2,HIGH);
  digitalWrite(bit3,HIGH);
  digitalWrite(bit4,LOW);
  delay(tiempo);
  digitalWrite(bit1,HIGH);
  digitalWrite(bit2,HIGH);
  digitalWrite(bit3,HIGH);
  digitalWrite(bit4,LOW);
  delay(tiempo);
  digitalWrite(bit1,LOW);
  digitalWrite(bit2,LOW);
  digitalWrite(bit3,LOW);
  digitalWrite(bit4,HIGH);
  delay(tiempo);
  digitalWrite(bit1,HIGH);
  digitalWrite(bit2,LOW);
  digitalWrite(bit3,LOW);
  digitalWrite(bit4,HIGH);
  delay(tiempo);
}
